<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "org.mehaexample.asdDemo.model.alignpublic.SingleValueAggregatedData" table="SingleValueAggregatedData">

        <meta attribute = "class-description">
            This class contains the Analytic Details for public database.
        </meta>
        <id name = "analyticKey" column="AnalyticKey" type="string">
            <generator class="assigned"/>
        </id>

        <property name = "analyticValue" column="AnalyticValue" type="int" />

    </class>
</hibernate-mapping>